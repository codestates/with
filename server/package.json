{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "nodemon index.js",
    "debug": "node --inspect index.js",
    "dev": "concurrently \"npm run start\" \"npm run start --prefix ../Client\"",
